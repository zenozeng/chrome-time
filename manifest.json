{
    "background": {
	"scripts": ["js/func.js","js/org-clock.js","js/background.js"]
    },
    "browser_action": {
	"default_icon": "icons/on.png",
	"default_popup": "popup.html"
    },
    "description": "一个简单的时间统计插件",
    "icons": {
	"128": "icons/on.png"
	// "16": "icons/icon16.png",
	// "48": "icons/icon48.png"
    },
    "manifest_version": 2,
    "name": "ChromeClock",
    "options_page": "options.html",
    "permissions": [
	"notifications",
	"tabs",
	"<all_urls>",
	"unlimitedStorage"
    ],
    "version": "0.0.1",
    "web_accessible_resources": [
    ]
}
